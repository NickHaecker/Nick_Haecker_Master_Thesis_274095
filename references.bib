@article{colombo_theoretical_2004,
	title = {Theoretical {Perspectives} in {Media}-{Communication} {Research}: {From} {Linear} to {Discursive} {Models}},
	volume = {5},
	copyright = {Copyright (c) 2004 Monica Colombo},
	issn = {1438-5627},
	shorttitle = {Theoretical {Perspectives} in {Media}-{Communication} {Research}},
	url = {https://www.qualitative-research.net/index.php/fqs/article/view/592},
	doi = {10.17169/fqs-5.2.592},
	abstract = {In diesem Beitrag soll ein Überblick über traditionelle lineare vs. dialogisch-diskursive Modelle in der Medienkommunikationsforschung gegeben werden. Die Beschäftigung mit Prozessen von Medienkommunikation hat sich mit der Evolution neuer theoretischer Modelle in der Psychologie und den Sozialwissenschaften verändert. Die Hauptprobleme traditioneller Modelle, die zunächst in der Medienforschung entwickelt worden waren, resultierten einerseits aus der rigiden Gegenüberstellung von "Sender" und "Empfänger" und andererseits aus der fehlenden Integration sozialer Variablen für eine angemessene Konzeptualisierung des Kommunikationsprozesses. Es wird gezeigt, wie die wachsende Beachtung des interaktiven Charakters von Kommunikation zu einer Ablösung der traditionellen linearen Modelle durch komplexere Modelle führte, verbunden mit einer Re-Definition des Kommunikationskonzepts wie z.B. in dialogischen Modellen und im Rahmen diskursiver Ansätze. Aus dieser Perspektive kommt den Medien eine zentrale Rolle für Prozesse der Wirklichkeitskonstruktion und -rekonstruktion zu; die Entwicklung angemessener qualitativer Verfahren ist in diesem Zusammenhang unabdingbar.
URN: urn:nbn:de:0114-fqs0402261},
	language = {en},
	number = {2},
	urldate = {2025-07-18},
	journal = {Forum Qualitative Sozialforschung / Forum: Qualitative Social Research},
	author = {Colombo, Monica},
	month = may,
	year = {2004},
	note = {Number: 2},
	keywords = {dialogical models, discursive approach, media communication process, media effects},
}

@article{colombo_theoretical_2004-1,
	title = {Theoretical {Perspectives} in {Media}-{Communication} {Research}: {From} {Linear} to {Discursive} {Models}},
	volume = {5},
	issn = {1438-5627},
	shorttitle = {Theoretical {Perspectives} in {Media}-{Communication} {Research}},
	abstract = {In diesem Beitrag soll ein Überblick über traditionelle lineare vs. dialogisch-diskursive Modelle in der Medienkommunikationsforschung gegeben werden. Die Beschäftigung mit Prozessen von Medienkommunikation hat sich mit der Evolution neuer theoretischer Modelle in der Psychologie und den Sozialwissenschaften verändert. Die Hauptprobleme traditioneller Modelle, die zunächst in der Medienforschung entwickelt worden waren, resultierten einerseits aus der rigiden Gegenüberstellung von "Sender" und "Empfänger" und andererseits aus der fehlenden Integration sozialer Variablen für eine angemessene Konzeptualisierung des Kommunikationsprozesses. Es wird gezeigt, wie die wachsende Beachtung des interaktiven Charakters von Kommunikation zu einer Ablösung der traditionellen linearen Modelle durch komplexere Modelle führte, verbunden mit einer Re-Definition des Kommunikationskonzepts wie z.B. in dialogischen Modellen und im Rahmen diskursiver Ansätze. Aus dieser Perspektive kommt den Medien eine zentrale Rolle für Prozesse der Wirklichkeitskonstruktion und -rekonstruktion zu; die Entwicklung angemessener qualitativer Verfahren ist in diesem Zusammenhang unabdingbar.This paper aims to provide an overview on traditional linear models vs. dialogical and discursive approaches to the study of mass media communication processes. The ways of conceiving the process of media-communication have changed along with the evolution of the theoretical paradigms of psychology and the social sciences. The main limits of these models which were first developed in the area of media studies derive, on the one hand, from the rigid alternating between source and receivers, and, on the other hand, from the lack of integration of the social variables in the conceptualization of the communication process. This paper will show how an increased focus on the interactive aspects of communication leads to the replacement of traditional linear models with more complex models implying a redefining of the concept of communication, as, for example, the dialogical models and the discursive approach. It is in this perspective that the media is seen to have a fundamental role in the processes of constructing/reconstructing reality, and the development of qualitative methodologies is especially needed.Este artículo intenta proporcionar una visión general sobre la oposición entre los modelos lineales tradicionales y los enfoques discursivos en el estudio de los procesos de los medios de comunicación de masas. Las maneras de concebir tales procesos han cambiado con la evolución de los paradigmas teóricos de la psicología y de las ciencias sociales. Las principales limitaciones de éstos modelos, que fueron primero desarrollados en el área de los estudios de los medios, provienen, por un lado, de la rígida alternancia entre fuente y receptores, y, por el otro, de la falta de integración de las variables sociales en la conceptualización de los procesos de comunicación. Este artículo muestra cómo un creciente interés en los aspectos interactivos de la comunicación, conduce a la sustitución de los modelos lineales tradicionales por modelos más complejos implicando una redefinición del concepto de comunicación; como por ejemplo, en los modelos dialógicos y el enfoque discursivo. Es en esta perspectiva que se ve el papel fundamental de los medios en los procesos de construcción y reconstrucción de la realidad, para lo cual es especialmente necesario el desarrollo de metodología cualitativas.},
	language = {en},
	number = {2},
	journal = {Forum Qualitative Sozialforschung / Forum: Qualitative Social Research},
	author = {Colombo, Monica},
	year = {2004},
	keywords = {Diskurs, Kommunikationsforschung, Medien, communication research, discourse, linear model, lineares Modell, media},
	pages = {11},
}

