	title = {Cooperative {Games} and {Their} {Effect} on {Group} {Collaboration}},
	isbn = {978-3-642-38827-9},
	doi = {10.1007/978-3-642-38827-9_43},
	abstract = {The potential for multiplayer computer games to serve as activities that can help increase interaction, cooperative tendencies and harmony in groups has been the subject of past research. However, there is still a long way to go before we can understand how positive group behavior and team dynamics in multiplayer games can impact real world collaboration. In our research work, we investigate this relationship further through Operation Sting, a cooperative multiplayer game we have designed to serve as an ice-breaker. Our goal is to study how participation in such a game affects collaboration in subsequent group work.},
	language = {en},
	booktitle = {Design {Science} at the {Intersection} of {Physical} and {Virtual} {Design}},
	publisher = {Springer},
	author = {Nasir, Maaz and Lyons, Kelly and Leung, Rock and Moradian, Ali},
